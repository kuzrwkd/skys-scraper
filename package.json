{
  "name": "skys-scraper",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "node_modules/.bin/rimraf dist",
    "build": "node_modules/.bin/nest build",
    "format": "node_modules/.bin/prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "start": "node_modules/.bin/nest start",
    "start:dev": "node_modules/.bin/nest start --watch",
    "start:debug": "node_modules/.bin/nest start --debug --watch",
    "start:prod": "node dist/Products/Server/main.js",
    "lint": "node_modules/.bin/eslint '{src,apps,libs,test}/**/*.ts'",
    "fix": "node_modules/.bin/eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "node_modules/.bin/jest",
    "test:watch": "node_modules/.bin/jest --watch",
    "test:cov": "node_modules/.bin/jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "node_modules/.bin/jest --config ./jest-e2e.json",
    "dynamodb:migration:up": "node dist/repository/db/migration/up.js",
    "dynamodb:migration:down": "node dist/repository/db/migration/down.js",
    "dynamodb:seed": "node dist/repository/db/seeder/install.js",
    "logs": "kubectl logs deployment/skys-scraper-app-dep -f"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.82.0",
    "@aws-sdk/lib-dynamodb": "^3.82.0",
    "@nestjs/common": "^8.4.4",
    "@nestjs/core": "^8.4.4",
    "@nestjs/platform-express": "^8.4.4",
    "dayjs": "^1.10.5",
    "puppeteer": "^13.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "tsyringe": "^4.5.0",
    "uuid": "^8.3.2",
    "verbal-expressions": "^1.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.10",
    "@nestjs/testing": "^8.4.4",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.23",
    "@types/puppeteer": "^5.4.3",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^28.0.3",
    "prettier": "^2.3.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.6.3"
  }
}
