{
  "name": "skys-scraper",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "node_modules/.bin/rimraf dist",
    "build": "node_modules/.bin/nest build",
    "format": "node_modules/.bin/prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "start": "node_modules/.bin/nest start",
    "start:dev": "node_modules/.bin/nest start --watch",
    "start:debug": "node_modules/.bin/nest start --debug --watch",
    "start:prod": "node dist/Products/Driver/Server/main.js",
    "start:batch": "node dist/Products/Driver/Batch/index.js",
    "lint": "node_modules/.bin/eslint '{src,apps,libs,test}/**/*.ts'",
    "fix": "node_modules/.bin/eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "node_modules/.bin/jest",
    "test:watch": "node_modules/.bin/jest --watch",
    "test:cov": "node_modules/.bin/jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "node_modules/.bin/jest --config ./jest-e2e.json",
    "dynamodb:migration": "node dist/Products/Driver/DB/Migration/NewsFeedTable.js",
    "dynamodb:describe": "aws dynamodb describe-table --table-name NewsFeed --endpoint-url http://skys-scraper-db-svc:8000",
    "dynamodb:cleanup": "aws dynamodb delete-table --table-name NewsFeed --endpoint-url http://skys-scraper-db-svc:8000",
    "logs": "kubectl logs deployment/skys-scraper-app-dep -f"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.44.0",
    "@nestjs/common": "^8.0.4",
    "@nestjs/core": "^8.0.4",
    "@nestjs/platform-express": "^8.0.4",
    "dayjs": "^1.10.5",
    "puppeteer": "^13.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "tsyringe": "^4.5.0",
    "uuid": "^8.3.2",
    "verbal-expressions": "^1.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.2",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.4",
    "@types/express": "^4.17.12",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.4.3",
    "@types/puppeteer": "^5.4.3",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.5"
  }
}
