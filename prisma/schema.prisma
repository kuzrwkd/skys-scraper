generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("PRISMA_DATABASE_URL")
}

model NewsFeed {
  id Int @id @default(autoincrement())
  url String @unique
  title String
  articleCreatedAt String
  articleUpdatedAt String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
